#include "BloockStatus.h"

string getBloockStatusString(BlockStatus bs) {
	if (bs == BloockStatus::BLOCK_FREE)
		return "free";
	else if (bs == BloockStatus::BLOCK_INVALID)
		return "invalid";
	
	return "valid";
}